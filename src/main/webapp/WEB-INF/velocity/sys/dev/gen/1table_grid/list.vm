#set($page_title="单表grid生成")
#set($selected_menu_id="menu_generator_1table_grid")

<div class="main-content">
    <div class="breadcrumbs" id="breadcrumbs">
        <script type="text/javascript">
            try {
                ace.settings.check('breadcrumbs', 'fixed')
            } catch (e) {
            }
        </script>

        <ul class="breadcrumb">
            <li>
                <i class="icon-home home-icon"></i>
                <a href="/sys/index.html">首页</a>
            </li>

            <li class="active">单表grid生成</li>
        </ul>
        <!-- .breadcrumb -->

    </div>

    <div class="page-content">
        <div class="page-header">
            <h1>单表grid生成</h1>
        </div>

        <div class="row">
            <div class="col-lg-12">
                <table class="table table-bordered ">
                    <tr>
                        <th>Name</th>
                        <th>CName</th>
                        <th>Java</th>
                        <th>Velocity</th>
                        <th>Javascript</th>
                    </tr>
                    #foreach($table in $tableList)
                        <tr>
                            <td><a href="/dev/generator/table/edit.html?name=${table.name}"> ${table.name}</a></td>
                            <td>$!{table.cname}</td>
                            <td>
                                <a href="/dev/generator/1table_grid/domain.html?name=${table.name}"  target="_blank">${table.domainPkg}.$!{table.domainClass}</a><br/>
                                <a href="/dev/generator/1table_grid/dao.html?name=${table.name}"  target="_blank">${table.daoPkg}.$!{table.daoClass}</a><br/>
                                <a href="/dev/generator/1table_grid/controller.html?name=${table.name}"  target="_blank">${table.controllerPkg}.$!{table.controllerClass}</a>
                            </td>
                            <td>
                                <a href="/dev/generator/1table_grid/list.vm.html?name=${table.name}" target="_blank">$!{table.urlPrefix}list.vm</a><br/>
                                <a href="/dev/generator/1table_grid/add.vm.html?name=${table.name}" target="_blank">$!{table.urlPrefix}add.vm</a><br/>
                                <a href="/dev/generator/1table_grid/edit.vm.html?name=${table.name}" target="_blank">$!{table.urlPrefix}edit.vm</a>
                            </td>
                            <td>
                                <a href="/dev/generator/1table_grid/list.js.html?name=${table.name}" target="_blank">$!{table.urlPrefix}list.js</a><br/>
                                <a href="/dev/generator/1table_grid/add.js.html?name=${table.name}" target="_blank">$!{table.urlPrefix}add.js</a><br/>
                                <a href="/dev/generator/1table_grid/edit.js.html?name=${table.name}" target="_blank">$!{table.urlPrefix}edit.js</a>
                            </td>
                        </tr>
                    #end
                </table>
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.page-content -->
</div><!-- /.main-content -->